{
    "name": "midnite81/salesforce",
    "description": "A PHP integration to Salesforce Api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Rogers",
            "email": "webdev@centralblue.co.uk"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework" : ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Midnite81\\Salesforce\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Midnite81\\Salesforce\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra" : {
        "laravel" : {
            "providers" : [
                "Midnite81\\Salesforce\\SalesforceServiceProvider"
            ]
        }
    }
}